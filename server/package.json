{
  "name": "webcatalog-server",
  "version": "0.0.1",
  "description": "Backend server for WinCatalog web UI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "wincatalog",
    "search",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@eslint/js": "^9.39.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "tsx": "^4.7.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.18"
  }
}
